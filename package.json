    "@nrwl/web": "13.8.5",
    "@nrwl/workspace": "13.8.5",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.1",
