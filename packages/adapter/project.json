{
  "name": "adapter",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/adapter/src",
  "projectType": "library",
  "targets": {
    "buildMeteorWallet": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "packages/adapter/dist/meteor-wallet"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/adapter/**/*.ts"
        ]
      }
    }
  },
  "tags": []
}